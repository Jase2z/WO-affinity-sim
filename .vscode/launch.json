    "version": "0.2.0",
    "configurations": [
        {
            "name": "Terminal",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "subProcess": true,
            "args":["-Xfrozen_modules=off"],
            "pythonArgs": ["-Xfrozen_modules=off"],
            "env": {"PYDEVD_DISABLE_FILE_VALIDATION":"1"}
        },
    ]
}